// Write a program to print the following pattern:
1
12
123
1234
12345

#include <stdio.h>

int main() {
    int rows = 5;

    for(int i = 1; i <= rows; i++) {         // loop for each row
        for(int j = 1; j <= i; j++) {        // print numbers from 1 to i
            printf("%d", j);
        }
        printf("\n");  // move to next line after each row
    }

    return 0;
}
