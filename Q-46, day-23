//  program to print the following pattern:
// *****
// *****
// *****
// *****
// *****

#include <stdio.h>

int main() {
    int rows = 5;

    for(int i = 1; i <= rows; i++) {         // loop for each row
        for(int j = 1; j <= rows; j++) {     // loop for each column
            printf("*");
        }
        printf("\n");  // move to next line after each row
    }

    return 0;
}
