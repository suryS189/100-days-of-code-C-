// rogram to print the product of even numbers from 1 to n.

#include <stdio.h>

int main() {
    int n;
    long long product = 1;   // use long long to handle large values

    printf("Enter n: ");
    scanf("%d", &n);

    for(int i = 2; i <= n; i += 2) {   // start from 2, step by 2
        product *= i;
    }

    printf("Product of even numbers from 1 to %d = %lld\n", n, product);

    return 0;
}
