// program to print the following pattern:
5
45
345
2345
12345

#include <stdio.h>

int main() {
    int rows = 5;

    for(int i = rows; i >= 1; i--) {         // starting number decreases each row
        for(int j = i; j <= rows; j++) {     // print numbers from i to 5
            printf("%d", j);
        }
        printf("\n");  // move to next line after each row
    }

    return 0;
}
